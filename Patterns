// 1) Square pattern
      
      int main()
      {
        int n;
        cin>>n;
    
        int i = 1;
        int j = 1;
    
        while(i<=n)
        {
          while(j<=n)
          {
            cout<<"* ";
            j +=1;
          }
          cout<<""<<endl;
          i +=1;
          j=1;
        }
        return 0;
      }
